msgid "plugins.importexport.pubmed.displayName"
msgstr "PubMed XML eksporto papildinys"

msgid "plugins.importexport.pubmed.description"
msgstr ""
"Eksportuoti straipsnių metaduomenis PubMed XML formatu indeksavimui MEDLINE "
"duomenų bazėje."

msgid "plugins.importexport.pubmed.export"
msgstr "Eksportuoti duomenis"

msgid "plugins.importexport.pubmed.export.issues"
msgstr "Eksportuoti numerius"

msgid "plugins.importexport.pubmed.export.articles"
msgstr "Eksportuoti straipsnius"

msgid "plugins.importexport.pubmed.export.selectArticle"
msgstr "Pasirinkite straipsnius eksportui."

msgid "plugins.importexport.pubmed.cliUsage"
msgstr ""
"Naudojimas:\n"
"{$scriptName} {$pluginName} [xmlFailoPavadinimas] [žurnalo_kelias] articles "
"[straipsnioId1] [straipsnioId2] ...\n"
"{$scriptName} {$pluginName} [xmlFailoPavadinimas] [žurnalo_kelias] issue "
"[numerioId]\n"

msgid "plugins.importexport.pubmed.cliError"
msgstr "KLAIDA:"

msgid "plugins.importexport.pubmed.export.error.issueNotFound"
msgstr "Nė vienas numeris neatitiko nurodyto numerio ID „{$issueId}“."

msgid "plugins.importexport.pubmed.export.error.articleNotFound"
msgstr "Nė vienas straipsnis neatitiko nurodyto straipsnio ID „{$articleId}“."

msgid "plugins.importexport.pubmed.settings.form.nlmTitle"
msgstr "NLM pavadinimo santrumpa"

msgid "plugins.importexport.pubmed.export.selectIssue"
msgstr "Pasirinkite numerį eksportui."

msgid "plugins.importexport.pubmed.settings.form.nlmTitle.description"
msgstr ""
"Žurnalo NLM pavadinimo santrumpa. Jei nežinote santrumpos, <a href=\"https"
